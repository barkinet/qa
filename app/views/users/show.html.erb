<div class="page-header">
  <h1><%= @user.name %></h1>
</div>

<div class="question-list user-question-list">
  <h3>Questions</h3>
  <ul>
    <% @questions.each do |question| %>
      <li>
        <span class="vote-count"><%= question.vote_count %></span>
        <%= link_to question.title, question %>
      </li>
    <% end %>
  </ul>
</div>